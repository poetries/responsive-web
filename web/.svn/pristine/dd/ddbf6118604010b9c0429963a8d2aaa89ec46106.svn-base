}
@media screen and (min-width: 992px) {
  .footer .col-xs-6.col-sm-6 {
    width: 20%;
  }
}
@media screen and (max-width: 992px) {
  .footer .col-xs-6.col-sm-6:nth-child(3),
  .footer .col-xs-6.col-sm-6:nth-child(4) {
