  }
}
/* columns */
.section-content {
  margin: 0 auto;
  padding: 120px 0;
}
.section-content h1 {
  margin-bottom: 20px;
}
.section-content h2,
.section-content h3 {
  margin: 0 0 24px;
  color: #333;
  font-weight: 400;
  background-repeat: no-repeat;
  background-position: center top;
}
.section-content h3 {
  padding-top: 120px;
}
.section-content a {
  margin-top: 30px;
}
.section-content.bg-gray-deep h2,
.section-content.bg-gray-deep h3 {
  padding-top: 50px;
}
.section-content .section-title {
  margin-bottom: 50px;
}
@media only screen and (max-device-width: 480px) {
  .section-content h2 {
    padding-top: 20px;
  }
}
@media (max-width: 767px) {
  .section-content .col-xs-12 {
    margin-bottom: 50px;
  }
}
/* well */
.well {
  margin-bottom: 0;
  background: #fff;
  border: none;
  border-radius: 0;
  box-shadow: none;
}
.well .well-content {
  background: #f6f6f6;
  border-left: none;
  border-right: none;
  margin-left: -20px;
  margin-right: -20px;
  padding: 20px 100px;
  border-top: 1px solid #b7b7b7;
  border-bottom: 1px solid #b7b7b7;
}
.well .well-content p {
  text-indent: 2em;
}
.well .well-content .block {
  margin-left: 20px;
  margin-right: 20px;
}
.well h1 {
  margin-bottom: 50px;
}
.well h2 {
  margin-top: 50px;
}
.well .row {
  margin-bottom: 20px;
}
.well h2,
.well h3 {
  font-weight: 400;
}
.well a {
  text-decoration: underline;
}
.well img {
  max-width: 85%;
  margin: 20px auto;
}
@media screen and (max-width: 767px) {
  .well .well-content {
    padding: 20px 30px;
  }
}
/* block */
.block {
  position: relative;
}
.block div {
  background: #05271a;
  width: 31%;
  margin: 0 1%;
  padding: 10px 10px 10px 100px;
  color: #dddfdf;
  background-repeat: no-repeat;
  background-position: 5% center;
  min-height: 90px;
}
.block h3 {
  color: #dddfdf;
  margin: 20px auto;
}
.block p {
  text-indent: 0 !important;
}
.block.block-2 div {
  background: #05271a;
  width: 48%;
  margin: 0 1%;
  padding: 10px 10px 10px 100px;
  color: #dddfdf;
  background-repeat: no-repeat;
  background-position: 5% center;
  min-height: 80px;
}
.block.block-padding-0 div {
  padding-left: 10px;
  text-align: left;
}
@media (max-width: 767px) {
  .block div {
    width: 95%;
    margin-top: 10px;
    margin-bottom: 20px;
  }
  .block.block-2 div {
    width: 95%;
    margin-top: 10px;
    margin-bottom: 20px;
  }
}
/*.triangle{*/
.block > div:before {
  content: "";
  position: absolute;
  left: -15px;
  top: 43%;
  display: inline-block;
  width: 0;
  height: 0;
  border: 10px solid;
  border-color: transparent #05271a transparent transparent;
}
.block-light div {
  background: #eee;
  color: #05271a;
}
.block-light > div:before {
  display: none;
}
/* leaf */
.leaf {
  padding: 22px 75px;
}
.leaf:after {
  content: "";
  position: absolute;
  margin: 18px auto 0;
  left: 50%;
  display: inline-block;
  width: 0;
  height: 0;
  border: 10px solid;
  border-color: #05271a transparent transparent transparent;
}
.leaf-primary {
  background-color: #05271a;
  color: #dcdcdc;
}
.leaf-primary:after {
  border-color: #05271a transparent transparent transparent;
}
.leaf-info {
  background-color: #ddd;
  color: #535353;
}
.leaf-info:after {
  border-color: #ddd transparent transparent transparent;
}
.leaf-no-triangle:after {
  border-color: transparent;
}
/* breadcrumb */
.breadcrumb {
  margin-bottom: 0;
  padding-bottom: 0;
}
.breadcrumb > li {
  color: #808080;
}
.breadcrumb > li:nth-of-type(1) > a:nth-of-type(1) {
