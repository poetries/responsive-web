/* labs */
.labs .banner {
  background-image: url(../img/banner/labs.jpg);
}
.labs .banner .title {
  background-image: url(../img/icon/labs.png);
  padding-top: 140px;
  margin-top: -70px;
}
.labs .section-content .row {
  margin-bottom: 80px;
}
.labs .section-content .pre-dot {
  float: left;
  display: inline;
  margin-left: 15px;
  margin-top: -20px;
  font-family: verdana;
  color: #a0a0a0;
}
.labs .section-content .button {
  padding: 0 2px;
  border: 1px solid #b8b8b8;
  border-radius: 40px;
  height: 40px;
  line-height: 40px;
}
.labs .section-content .button span {
  display: block;
  border: 1px solid #b8b8b8;
  border-radius: 34px;
  height: 34px;
  line-height: 34px;
  margin-top: 2px;
}
.labs .section-content .wrap-content {
  text-align: left;
  padding-left: 150px;
  height: 120px;
}
.labs .section-content .content-agile {
  background: url(../img/icon/labs/agile.png) no-repeat;
}
.labs .section-content .content-abtesting {
  background: url(../img/icon/labs/abtesting.png) no-repeat;
}
.labs .section-content .content-tdd {
  background: url(../img/icon/labs/tdd.png) no-repeat;
}
.labs .section-content .content-gray {
  background: url(../img/icon/labs/gray.png) no-repeat;
}
/* team */
.team section.contact {
  padding: 50px 0;
}
.team section.bg-gray-deep h2 {
  background-image: url(/assets/img/home/line_gray.png);
}
.team section h2 {
  background: url(/assets/img/home/line_orange.png) no-repeat 0 100%;
  padding-bottom: 10px;
}
.team section h2.text-center {
  background-position: 50% 100%;
}
.team .section-content h3 {
  padding-top: 50px;
}
.team .banner {
  background-image: url(../img/about/bg_banner.jpg);
}
.team .intro {
  background: url(/assets/img/home/bg_earth.png) no-repeat 90% 90%;
}
.team .intro h2 {
  font-size: 26px;
  font-weight: 400;
  margin: 50px auto 50px;
  color: #313131;
}
.team .intro div h2:before {
  display: block;
  width: 20px;
  border-bottom: 3px solid #eb872e;
}
.team .intro p {
  font-size: 18px;
}
.contact__title {
  font-weight: 300;
  font-size: 100%;
  padding: 10px 0;
}
.contact__desc {
  font-weight: 700;
  font-size: 120%;
  margin-bottom: 36px;
}
.contact__title--address {
  background: url(../img/about/icon_location.png) no-repeat 38% 0;
}
.contact__title--email {
  background: url(../img/about/icon_email.png) no-repeat 38% 10px;
}
.contact__title--business {
  background: url(../img/about/icon_tel.png) no-repeat 38% 5px;
}
.contact__title--tech {
  background: url(../img/about/icon_tel.png) no-repeat 38% 5px;
}
@media (min-width: 992px) {
  .contact__title {
    background-position-x: 42%;
  }
}
@media (min-width: 1200px) {
  .contact__title {
    background-position-x: 45%;
  }
}
@media (max-width: 420px) {
  .contact__title {
    background-position-x: 36%;
  }
}
@media (max-width: 360px) {
  .contact__title {
    background-position-x: 34%;
  }
}
