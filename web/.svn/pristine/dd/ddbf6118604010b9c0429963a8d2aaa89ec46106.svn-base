  bottom: 30px;
  margin: 0 auto;
}
.footer .contact-us {
  background: #333 url(/assets/img/home/icons_contact.png) no-repeat 30px 75px;
  padding-bottom: 120px;
  padding-left: 50px;
}
.footer .contact-us:after {
  background: red;
  display: block;
  width: 3000px;
  height: 2000px;
}
.footer .contact-us p {
  padding-left: 45px;
