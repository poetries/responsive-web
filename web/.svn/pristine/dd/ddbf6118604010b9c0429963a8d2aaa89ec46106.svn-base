body {
  font-size: 14px;
  font-weight: 300;
  color: #777;
  background: #fff;
}
body,
h1,
h2,
h3,
h4,
h5,
h6,
.h1,
.h2,
.h3,
.h4,
.h5,
.h6,
.btn {
  font-family: "Microsoft YaHei", Helvetica, STHeiti, Droid Sans Fallback;
}
h1,
.h1 {
  font-weight: 400;
  font-size: 26px;
}
h2,
.h2 {
  font-size: 26px;
}
h3,
.h3 {
  font-size: 20px;
}
section {
  background-color: #fff;
  overflow: hidden;
}
section .content {
  margin: 120px auto;
}
a,
a:hover,
a:visited {
  text-decoration: none;
  color: #313131;
}
.btn,
.btn:hover,
.btn:visited {
  background: #fff;
  color: #eb872e;
  border-radius: 5px;
  border-color: #c9c9c9;
  padding: 5px 20px;
}
.btn:hover {
  background: #eb872e;
  color: #fff;
  border-color: transparent;
}
a.btn-primary,
a.btn-primary:hover {
  background: #05271a;
  border: 1px solid #91a09a;
  padding: 5px 20px;
  color: #fff;
}
a.btn-default,
a.btn-default:hover {
  background: #808080;
  border: 1px solid #91a09a;
  padding: 5px 20px;
  color: #fff;
}
a.btn-lg,
a.btn-lg:hover,
a.btn-lg:visited {
  border: 2px solid #eb872e;
  padding: 10px 20px;
}
body > .container {
  padding-left: 0;
  padding-right: 0;
}
@media (min-width: 768px) {
  .container {
    width: 100%;
  }
}
@media (min-width: 992px) {
  .container {
    width: 100%;
  }
}
@media (min-width: 1200px) {
  .container {
    max-width: 1920px;
    margin: 0 auto;
  }
}
.navbar {
  width: 100%;
  position: fixed;
  z-index: 9999;
  border: none;
  height: 60px;
  border-bottom: 5px solid #042217;
  border-radius: 0;
  margin-bottom: 0;
}
.navbar > .container .navbar-brand {
  margin-left: 0;
}
.navbar a[target="_blank"] {
  background: url(/assets/home/img/icon/link.png) no-repeat 95% 5px;
}
.navbar-toggle {
  margin-top: 7px;
}
.navbar-nav {
  padding-top: 10px;
  margin-top: 0;
}
.navbar-nav .open > a:hover {
  color: #fff;
}
.navbar-default {
  border-bottom: 2px solid #555;
}
.navbar-default .navbar-brand {
  background: url(/assets/img/logo-white.png) no-repeat 0 10px;
  background-size: 253px 36px;
  width: 263px;
  height: 46px;
  text-indent: -999em;
  overflow: hidden;
}
.navbar-default .navbar-collapse .nav {
  background: #fff;
}
.navbar-default .navbar-nav > li > a,
.navbar-default .navbar-nav > li > a:hover,
.navbar-default .navbar-nav > li > a:focus,
.navbar-default .navbar-nav > .active > a,
.navbar-default .navbar-nav > .active > a:hover,
.navbar-default .navbar-nav > .active > a:focus,
.navbar-default .navbar-nav > .open > a,
.navbar-default .navbar-nav > .open > a:hover,
.navbar-default .navbar-nav > .open > a:focus {
  text-decoration: none;
  color: #1b1b1b;
  color: #333;
}
@media only screen and (-webkit-min-device-pixel-ratio: 2) {
  .navbar-default .navbar-brand {
    background-image: url(/assets/img/logo-white@2x.png);
  }
}
@media (max-width: 767px) {
  .navbar-default .navbar-nav .open .dropdown-menu > li.open > a {
    color: #fff;
  }
}
.navbar-transparent .navbar-brand {
  background: url(/assets/img/logo.png) no-repeat 0 10px;
  background-size: 253px 36px;
  width: 263px;
  height: 46px;
  text-indent: -999em;
  overflow: hidden;
}
.navbar-transparent.navbar {
  border: none;
  border-bottom: none;
  background-color: transparent;
}
.navbar-transparent .navbar-toggle {
  border-color: #aaa;
}
.navbar-transparent .navbar-toggle .icon-bar {
  background: #aaa;
}
.navbar-transparent .navbar-nav > li > a,
.navbar-transparent .navbar-nav > li > a:hover,
.navbar-transparent .navbar-nav > li > a:focus,
.navbar-transparent .navbar-nav > .active > a,
.navbar-transparent .navbar-nav > .active > a:hover,
.navbar-transparent .navbar-nav > .active > a:focus,
.navbar-transparent .navbar-nav > .open > a,
.navbar-transparent .navbar-nav > .open > a:hover,
.navbar-transparent .navbar-nav > .open > a:focus {
  color: #fff;
  background: transparent;
}
.navbar-transparent .navbar-collapse .nav {
  background: transparent;
}
@media only screen and (-webkit-min-device-pixel-ratio: 2) {
  .navbar-transparent .navbar-brand {
    background-image: url(/assets/img/logo@2x.png);
  }
}
@media only screen and (max-device-width: 480px) {
  .navbar {
    height: 50px;
  }
  .navbar > .container .navbar-brand {
    margin-left: 10px;
  }
}
@media only screen and (max-device-width: 480px) {
  .navbar-default .navbar-brand,
  .navbar-transparent .navbar-brand {
    width: 212px;
    height: 39px;
    background-size: 202px 29px;
  }
  .navbar-transparent .navbar-collapse .nav {
    background-color: #fff;
  }
  .navbar-transparent .navbar-nav > li > a,
  .navbar-transparent .navbar-nav > li > a:hover,
  .navbar-transparent .navbar-nav > li > a:focus,
  .navbar-transparent .navbar-nav > .active > a,
  .navbar-transparent .navbar-nav > .active > a:hover,
  .navbar-transparent .navbar-nav > .active > a:focus,
  .navbar-transparent .navbar-nav > .open > a,
  .navbar-transparent .navbar-nav > .open > a:hover,
  .navbar-transparent .navbar-nav > .open > a:focus {
    color: #333;
  }
}
.nav .open > a,
.nav .open > a:hover,
.nav .open > a:focus {
  background: #777;
  color: #fff;
}
.dropdown-menu > li > a {
  color: #1b1b1b;
}
.dropdown-menu > li > a:hover,
.dropdown-menu > li > a:focus {
  text-decoration: none;
  color: #ffffff;
  background-color: #eb872e;
}
.dropdown-menu > .active > a,
.dropdown-menu > .active > a:hover,
.dropdown-menu > .active > a:focus {
  color: #ffffff;
  text-decoration: none;
  outline: 0;
  background-color: #05271a;
}
.footer {
  background: #eb872e;
  color: #fff;
  position: relative;
}
.footer a {
  color: #efefef;
}
.footer a:hover {
  color: #fff;
}
.footer .footer-nav {
  padding: 0;
  max-width: 1920px;
  margin: 0 auto;
}
.footer h3 {
  margin-bottom: 30px;
  margin-top: 50px;
}
.footer h3,
.footer h3 a {
  font-size: 16px;
  font-weight: 400;
  color: #fff;
}
.footer h4 {
  font-size: 15px;
  font-weight: 400;
}
.footer p {
  font-size: 12px;
}
.footer .copyright {
  width: 90%;
